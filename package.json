{
  "name": "leafer-x-effect-text",
  "type": "module",
  "version": "1.6.1",
  "packageManager": "pnpm@10.14.0",
  "description": "Leafer 特效文字插件",
  "author": "XiaDeYu <1579883916@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/Xdy1579883916/leafer-x-effect-text#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xdy1579883916/leafer-x-effect-text.git"
  },
  "bugs": "https://github.com/Xdy1579883916/leafer-x-effect-text/issues",
  "keywords": [
    "leafer",
    "文字",
    "特效文字"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
