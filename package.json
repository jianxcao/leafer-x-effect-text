    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "prepare": "simple-git-hooks"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@antfu/ni": "^25.0.0",
    "@antfu/utils": "^9.3.0",
    "@lx/effect-text": "workspace:*",
    "@types/node": "^24.10.1",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.1",
    "lint-staged": "^16.2.7",
    "simple-git-hooks": "^2.13.1",
    "tinyexec": "^1.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.2.4",
    "vitest": "^3.2.4",
    "vitest-package-exports": "^0.1.1",
    "yaml": "^2.8.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
